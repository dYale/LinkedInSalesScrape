{
  "name": "LinkedIn - Sales Lead Assistant",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "The extension scrapes LinkedIn Sales Navigator and generates an Email List of leads that you can paste to excel.",
  "icons": {
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_title": "LinkedIn - Sales Lead Assistant",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "clipboardWrite",
    "http://www.linkedin.com",
    "https://www.linkedin.com",
    "http://linkedin.com",
    "https://linkedin.com"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/jquery/jquery.min.js",
        "src/inject/basescript.js"
      ]
    }
  ]
}